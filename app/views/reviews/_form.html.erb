<%= simple_form_for(@review) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :taste %>
    <%= f.input :aroma %>
    <%= f.input :appearance %>
    <%= f.input :headline %>
    <%= f.input :overall_score %>
    <%= f.input :like_dislike %>
    <%= f.input :review %>
    <%= f.association :user %>
    <%#= f.association :batch %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>


